{
  "server": {
    "authorization": {
      "enabled": false,
      "token": {
        "value": ""
      }
    },
    "general": {
      "port": 8080,
      "prefix": "/api",
      "loglevel": "trace"
    },
    "security": {
      "enabled": false,
      "cert": "",
      "key": ""
    }
  },
  "api": {
    "configFilePath": "",
    "modelsPath": "",
    "routes": [
      {
        "path": "/users",
        "model": {
          "name": {
            "first": "{{name.firstName}}",
            "last": "{{name.lastName}}"
          },
          "address": {
            "city": "{{address.city}}",
            "zipCode": "{{address.zipCode}}",
            "street": "{{address.streetAddress}}",
            "secondary": "{{address.secondaryAddress}}"
          },
          "job": {
            "company": "{{company.companyName}}",
            "title": "{{name.title}}",
            "type": "{{name.jobType}}"
          },
          "email": "{{internet.email}}",
          "phone": "{{phone.phoneNumberFormat}}"
        }
      }
    ]
  }
}
